# money
counting money
#include <stdio.h>
#include <conio.h>
void main(){
	int n, i, j, d, g = 0,h=0;
	int dem = 0;
	printf("nhap vao n=");
	scanf_s("%d", &n);
	i = n / 10;
	printf("%d 10 dong\n", i);
	j = n % 10;
	if (j > 4){
		dem = dem + 1;
		printf("%d 5 dong \n", dem);
	}
	else
	{
		printf("0 5 dong\n");
	};
	d = j - dem * 5;
	for (d; d > 1; )
	{
		g = g + 1;
		d = d - 2;
	}
	printf("%d 2 dong\n", g);
	h =j - dem*5-g*2;
	printf("%d 1 dong\n", h);

	_getch();

}
